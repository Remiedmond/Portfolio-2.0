		"gh-pages": "^6.3.0",
		"nodemon": "^3.1.10",
		"svelte": "^5.0.0",
		"vite": "^7.0.4"
	},
	"dependencies": {
		"gsap": "^3.13.0",
		"scrollreveal": "^4.0.9",
		"svelte-spa-router": "^4.0.1"
	}
