	"version": "0.0.1",
	"type": "module",
	"homepage": "https://Remiedmond.github.io/Portfolio-2.0",
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"prepare": "svelte-kit sync || echo ''",
